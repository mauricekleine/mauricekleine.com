{
  "name": "maurice-kleine-dot-com",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "homepage": "https://mauricekleine.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mauricekleine/mauricekleine.com"
  },
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./app.css -o app/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app.css -o app/app.css",
    "lint": "eslint ./app && prettier ./app --check",
    "lint:fix": "eslint ./app --fix && prettier ./app --write",
    "prepare": "husky install",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "~1.3.4",
    "@remix-run/react": "~1.3.4",
    "@remix-run/vercel": "~1.3.4",
    "@vercel/node": "~1.14.0",
    "classnames": "~2.3.1",
    "date-fns": "~2.28.0",
    "phosphor-react": "~1.4.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "~1.3.4",
    "@remix-run/eslint-config": "~1.3.4",
    "@remix-run/serve": "~1.3.4",
    "@tailwindcss/typography": "~0.5.2",
    "@types/node": "~17.0.23",
    "@types/react": "~17.0.43",
    "@types/react-dom": "~17.0.14",
    "autoprefixer": "~10.4.4",
    "concurrently": "~7.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-unused-imports": "~2.0.0",
    "husky": "~7.0.4",
    "lint-staged": "~12.3.7",
    "postcss": "~8.4.12",
    "prettier": "~2.6.2",
    "prettier-plugin-tailwindcss": "~0.1.8",
    "tailwindcss": "~3.0.23",
    "typescript": "~4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
