{
  "compilerOptions": {
    "baseUrl": "./",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "noEmit": true
  },
  "exclude": ["node_modules"],
  "extends": "tsconfig/next.json",
  "include": [
    "next.config.js",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
