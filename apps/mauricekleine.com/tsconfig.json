{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/lib": ["./src/lib/*"],
      "~/*": ["./src/features/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"],
  "extends": "tsconfig/next.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
